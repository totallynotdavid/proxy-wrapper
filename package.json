    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "dev": "ts-node -r tsconfig-paths/register src/dev.ts",
        "clean": "shx rm -rf dist node_modules pnpm-lock.yaml",
